/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP or3input {
    IN a,b,c;
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=a,b=b,out=out1);
    Or(a=out1,b=c,out=out);
    
    
    }